---
interface Props {
	title: string;
}

const { title } = Astro.props;

const navItems = [
	{ label: 'HOME', href: '/' },
	{ label: 'THE GLOB', href: '/the-glob' },
	{ label: 'HEALTH CHECK', href: '/health-check' },
	{ label: 'EVENTS', href: '/events' },
	{ label: 'CONTACT', href: '/contact' },
	{ label: 'SPONSORS', href: '/sponsors' },
];

const currentPath = Astro.url.pathname;
---

<!DOCTYPE html>
<html lang="en-GB" dir="ltr">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="author" content="Expose Yourself" />
	<meta name="description" content="A short description of your website content, what you talk about and who you are talking to. We suggest you add this text to tell your users what you are talking about in your website before they come and visit. It will be amongst search engine results." />
	<title>{title}</title>
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link href="https://fonts.googleapis.com/css2?family=Fredoka+One&family=Open+Sans:wght@400;700&display=swap" rel="stylesheet">
</head>
<body>
	<div id="imPageExtContainer">
		<div id="imPageIntContainer">
			<div id="imHeaderBg"></div>
			<div id="imFooterBg"></div>
			<div id="imPage">
				<header id="imHeader">
					<h1 class="imHidden">The Glob</h1>
					<div id="imHeaderObjects">
						<div id="imHeader_imMenuObject_12_wrapper">
							<div id="imHeader_imMenuObject_12">
								<div id="imHeader_imMenuObject_12_container">
									<div class="hamburger-button hamburger-component">
										<div><div>
											<div class="hamburger-bar"></div>
											<div class="hamburger-bar"></div>
											<div class="hamburger-bar"></div>
										</div></div>
									</div>
									<div class="hamburger-menu-background-container hamburger-component">
										<div class="hamburger-menu-background menu-mobile menu-mobile-animated hidden">
											<div class="hamburger-menu-close-button"><span>&times;</span></div>
										</div>
									</div>
									<ul class="menu-mobile-animated hidden">
										{navItems.map(item => (
											<li class={`imPage ${currentPath === item.href || (item.href !== '/' && currentPath.startsWith(item.href)) ? 'imMnMnCurrent' : ''}`} data-link-paths={item.href}>
												<div class="label-wrapper">
													<div class="label-inner-wrapper">
														<a class="label" href={item.href}>{item.label}</a>
													</div>
												</div>
											</li>
										))}
									</ul>
								</div>
							</div>
						</div>
						<div id="imHeader_imObjectTitle_13_wrapper">
							<div id="imHeader_imObjectTitle_13">
								<span id="imHeader_imObjectTitle_13_text">
									<a href="/">The Glob</a>
								</span>
							</div>
						</div>
						<div id="imHeader_imObjectImage_14_wrapper">
							<div id="imHeader_imObjectImage_14">
								<div id="imHeader_imObjectImage_14_container">
									<img src="/images/LCI_emblem_2color_web_685u20kc.png" alt="" width="138" height="138" />
								</div>
							</div>
						</div>
					</div>
				</header>
				
				<div id="imStickyBarContainer">
					<div id="imStickyBarGraphics"></div>
					<div id="imStickyBar">
						<div id="imStickyBarObjects">
							<div id="imStickyBar_imMenuObject_01_wrapper">
								<div id="imStickyBar_imMenuObject_01">
									<div id="imStickyBar_imMenuObject_01_container">
										<div class="hamburger-button hamburger-component">
											<div><div>
												<div class="hamburger-bar"></div>
												<div class="hamburger-bar"></div>
												<div class="hamburger-bar"></div>
											</div></div>
										</div>
										<div class="hamburger-menu-background-container hamburger-component">
											<div class="hamburger-menu-background menu-mobile menu-mobile-animated hidden">
												<div class="hamburger-menu-close-button"><span>&times;</span></div>
											</div>
										</div>
										<ul class="menu-mobile-animated hidden">
											{navItems.map(item => (
												<li class={`imPage ${currentPath === item.href || (item.href !== '/' && currentPath.startsWith(item.href)) ? 'imMnMnCurrent' : ''}`} data-link-paths={item.href}>
													<div class="label-wrapper">
														<div class="label-inner-wrapper">
															<a class="label" href={item.href}>{item.label}</a>
														</div>
													</div>
												</li>
											))}
										</ul>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>

				<a class="imHidden" href="#imGoToCont">Go to content</a>
				
				<div id="imSideBar">
					<div id="imSideBarObjects"></div>
				</div>
				
				<div id="imContentGraphics"></div>
				
				<main id="imContent">
					<a id="imGoToCont"></a>
					<slot />
				</main>
				
				<footer id="imFooter">
					<div id="imFooterObjects">
						<div id="imFooter_imObjectTitle_50_wrapper">
							<div id="imFooter_imObjectTitle_50">
								<span id="imFooter_imObjectTitle_50_text">
									<a href="/">The Glob</a>
								</span>
							</div>
						</div>
						<div id="imFooter_imObjectImage_51_wrapper">
							<div id="imFooter_imObjectImage_51">
								<div id="imFooter_imObjectImage_51_container">
									<img src="/images/weServe.png" alt="" width="150" height="82" />
								</div>
							</div>
						</div>
						<div id="imFooter_imTextObject_52_wrapper">
							<div id="imFooter_imTextObject_52">
								<div class="text-tab-content current-tab">
									<div class="text-inner">
										<span class="fs8lh1-5 cf1">Project run by Lions District 202D - Zone 1</span>
										<div><span class="fs8lh1-5 cf1">Dr Warren Nicholls</span></div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</footer>
			</div>
		</div>
	</div>
	
	<style is:global>
		/* Reset and Base */
		* { margin: 0; padding: 0; box-sizing: border-box; }

		html {
			position: relative;
			margin: 0;
			padding: 0;
			height: 100%;
			overflow-x: hidden;
		}

		body {
			margin: 0;
			padding: 0;
			text-align: left;
			height: 100%;
			font-family: 'Open Sans', sans-serif;
			overflow-x: hidden;
		}

		img {
			max-width: 100%;
			height: auto;
		}

		.imHidden { display: none; }
		
		/* Page Container */
		#imPageExtContainer {
			min-height: 100%;
			background-color: #ea7235;
		}
		
		#imPageIntContainer {
			position: relative;
			overflow-x: hidden;
		}

		#imPage {
			position: relative;
			width: 100%;
			display: grid;
			grid-template-columns: minmax(0, 1fr) 960px minmax(0, 1fr);
			grid-template-rows: 150px auto 120px;
		}
		
		/* Header */
		#imHeaderBg {
			position: absolute;
			left: 0;
			top: 0;
			width: 100%;
			height: 80px;
			background-color: #252628;
			z-index: 1;
		}
		
		#imHeader {
			display: block;
			grid-column: 2 / 3;
			grid-row: 1 / 2;
			position: relative;
			z-index: 10;
		}
		
		#imHeaderObjects {
			margin-left: auto;
			margin-right: auto;
			position: relative;
			height: 150px;
			width: 960px;
		}
		
		/* Menu in Header */
		#imHeader_imMenuObject_12_wrapper {
			position: absolute;
			top: 6px;
			left: -4px;
			width: 847px;
			height: 42px;
			text-align: center;
			z-index: 1002;
		}
		
		#imHeader_imMenuObject_12_container {
			border-style: solid;
			border-width: 0;
			width: 100%;
			box-sizing: border-box;
			background-color: transparent;
			padding: 0;
			display: inline-table;
		}
		
		#imHeader_imMenuObject_12_container > ul {
			list-style: none;
			margin: 0;
			padding: 0;
		}
		
		#imHeader_imMenuObject_12_container > ul:after {
			content: '';
			display: table;
			clear: both;
		}
		
		#imHeader_imMenuObject_12_container > ul > li {
			position: relative;
			margin-left: 0;
			margin-top: 0;
			display: inline-block;
			vertical-align: top;
		}
		
		#imHeader_imMenuObject_12_container > ul > li > .label-wrapper {
			display: block;
			overflow: hidden;
			width: 140px;
			height: 31px;
			padding: 0;
			border-width: 0;
			background-color: transparent;
			border-color: #d3d3d3;
			border-style: solid;
		}
		
		.label-inner-wrapper {
			display: table;
			width: 100%;
			height: 100%;
			position: relative;
			z-index: 1;
		}
		
		#imHeader_imMenuObject_12_container > ul > li .label {
			display: table-cell;
			vertical-align: middle;
			width: 100%;
			height: 100%;
			font-family: 'Open Sans', sans-serif;
			font-size: 12pt;
			font-style: normal;
			text-decoration: none;
			text-align: center;
			color: #ffffff;
			font-weight: normal;
		}
		
		#imHeader_imMenuObject_12_container > ul > li:hover > .label-wrapper .label,
		#imHeader_imMenuObject_12_container > ul > li.imMnMnCurrent > .label-wrapper .label {
			color: #ffbf00;
		}
		
		/* Title */
		#imHeader_imObjectTitle_13_wrapper {
			position: absolute;
			top: 89px;
			left: -1px;
			width: 221px;
			height: 60px;
			text-align: center;
			z-index: 1003;
		}
		
		#imHeader_imObjectTitle_13 {
			margin: 0;
			text-align: center;
		}
		
		#imHeader_imObjectTitle_13_text {
			font-family: 'Fredoka One', cursive;
			font-size: 24pt;
			padding: 0;
			background-color: transparent;
			position: relative;
			display: inline-block;
		}
		
		#imHeader_imObjectTitle_13_text a {
			color: #252628;
			text-decoration: none;
			white-space: pre-wrap;
		}
		
		/* Header Image */
		#imHeader_imObjectImage_14_wrapper {
			position: absolute;
			top: 12px;
			left: 811px;
			width: 138px;
			height: 135px;
			text-align: center;
			z-index: 1004;
			overflow: hidden;
		}
		
		#imHeader_imObjectImage_14_container img {
			width: 100%;
			height: auto;
			vertical-align: top;
		}
		
		/* Sticky Bar */
		#imStickyBarContainer {
			visibility: visible;
			position: fixed;
			top: 0;
			left: 0;
			right: 0;
			margin: auto;
			width: 100%;
			height: 100px;
			box-sizing: border-box;
			z-index: 10301;
			-webkit-transform: translateZ(0);
			transform: translateZ(0);
		}
		
		#imStickyBarGraphics {
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			margin: auto;
			width: 100%;
			height: 100px;
			box-sizing: border-box;
			background-color: #252628;
			opacity: 1;
			z-index: 10301;
		}
		
		#imStickyBar {
			position: relative;
			top: 0;
			left: 0;
			margin: auto;
			width: 960px;
		}
		
		#imStickyBarObjects {
			position: relative;
			height: 100px;
		}
		
		#imStickyBar_imMenuObject_01_wrapper {
			position: absolute;
			top: 10px;
			left: 10px;
			width: 897px;
			height: 42px;
			text-align: center;
			z-index: 1002;
		}
		
		#imStickyBar_imMenuObject_01_container {
			border-style: solid;
			border-width: 0;
			width: 100%;
			box-sizing: border-box;
			background-color: transparent;
			padding: 0;
			display: inline-table;
		}
		
		#imStickyBar_imMenuObject_01_container > ul {
			list-style: none;
			margin: 0;
			padding: 0;
		}
		
		#imStickyBar_imMenuObject_01_container > ul:after {
			content: '';
			display: table;
			clear: both;
		}
		
		#imStickyBar_imMenuObject_01_container > ul > li {
			position: relative;
			margin-left: 0;
			margin-top: 0;
			display: inline-block;
			vertical-align: top;
		}
		
		#imStickyBar_imMenuObject_01_container > ul > li > .label-wrapper {
			display: block;
			overflow: hidden;
			width: 140px;
			height: 33px;
			padding: 0;
			border-width: 0;
			background-color: transparent;
			border-color: #d3d3d3;
			border-style: solid;
		}
		
		#imStickyBar_imMenuObject_01_container > ul > li .label {
			display: table-cell;
			vertical-align: middle;
			width: 100%;
			height: 100%;
			font-family: 'Open Sans', sans-serif;
			font-size: 13pt;
			font-style: normal;
			text-decoration: none;
			text-align: center;
			color: #ffffff;
			font-weight: normal;
		}
		
		#imStickyBar_imMenuObject_01_container > ul > li:hover > .label-wrapper .label,
		#imStickyBar_imMenuObject_01_container > ul > li.imMnMnCurrent > .label-wrapper .label {
			color: #ffbf00;
		}
		
		/* Content Area */
		#imContentGraphics {
			display: block;
			grid-column: 2 / 3;
			grid-row: 2 / 3;
			min-height: 380px;
			background-color: #ea7235;
		}
		
		#imContent {
			display: block;
			grid-column: 1 / 4;
			grid-row: 2 / 3;
			position: relative;
			align-self: start;
			padding: 0;
		}
		
		/* Footer - Matches Original */
		#imFooterBg {
			position: absolute;
			left: 0;
			bottom: 0;
			width: 100%;
			height: 120px;
			background-color: #252628;
			z-index: 1;
		}

		#imFooter {
			display: block;
			grid-column: 2 / 3;
			grid-row: 3 / 4;
			position: relative;
			z-index: 10;
		}

		#imFooterObjects {
			margin-left: auto;
			margin-right: auto;
			position: relative;
			height: 120px;
			width: 960px;
		}

		/* Title - "The Glob" */
		#imFooter_imObjectTitle_50_wrapper {
			position: absolute;
			top: 24px;
			left: 26px;
			width: 134px;
			height: 24px;
			text-align: center;
			z-index: 1002;
		}

		#imFooter_imObjectTitle_50 {
			margin: 0;
			text-align: left;
		}

		#imFooter_imObjectTitle_50_text {
			font-family: 'Fredoka One', cursive;
			font-size: 20pt;
			padding: 0;
			background-color: transparent;
			position: relative;
			display: inline-block;
		}

		#imFooter_imObjectTitle_50_text a {
			color: #ffffff;
			text-decoration: none;
			white-space: pre-wrap;
		}

		/* Logo Image */
		#imFooter_imObjectImage_51_wrapper {
			position: absolute;
			top: 20px;
			left: 800px;
			width: 150px;
			height: 100px;
			text-align: center;
			z-index: 1003;
			overflow: hidden;
		}

		#imFooter_imObjectImage_51_container img {
			width: 100%;
			height: auto;
			vertical-align: top;
		}

		/* Text Content */
		#imFooter_imTextObject_52_wrapper {
			position: absolute;
			top: 52px;
			left: 26px;
			width: 510px;
			height: 60px;
			text-align: center;
			z-index: 1004;
			overflow: hidden;
		}

		#imFooter_imTextObject_52 {
			font-style: normal;
			font-weight: normal;
			line-height: 28px;
		}
		
		.fs8lh1-5 {
			font-size: 8pt;
			line-height: 16px;
		}
		
		.cf1 {
			color: #f3f3f3;
		}
		
		/* Content Styles */
		.imPageRow {
			position: relative;
			width: 960px;
			margin: 0 auto;
		}
		
		/* SideBar */
		#imSideBar {
			display: none;
		}
		
		/* Text formatting */
		.fs20lh1-5 {
			font-size: 20pt;
			line-height: 40px;
		}
		
		.fs16lh1-5 {
			font-size: 16pt;
			line-height: 32px;
		}
		
		.fs14lh1-5 {
			font-size: 14pt;
			line-height: 28px;
		}
		
		.imTACenter {
			text-align: center;
		}
		
		.imTALeft {
			text-align: left;
		}
		
		.imHeading4 {
			font-size: 18pt;
			font-weight: bold;
		}
		
		.ff1 {
			font-family: 'Calibri', sans-serif;
		}
		
		/* Links */
		.imCssLink {
			color: inherit;
			text-decoration: underline;
		}
		
		.imCssLink:hover {
			color: #ffbf00;
		}

		/* Hamburger Button - Hidden by default on desktop */
		#imHeader_imMenuObject_12 .hamburger-component,
		#imStickyBar_imMenuObject_01 .hamburger-component {
			display: none;
		}

		#imHeader_imMenuObject_12 .hamburger-button,
		#imStickyBar_imMenuObject_01 .hamburger-button {
			cursor: pointer;
			width: 31px;
			height: 31px;
			display: inline-block;
			vertical-align: top;
			padding: 0;
			border-width: 0;
		}

		#imHeader_imMenuObject_12 .hamburger-button > div,
		#imStickyBar_imMenuObject_01 .hamburger-button > div {
			display: table;
			width: 100%;
			height: 100%;
			position: relative;
			z-index: 1;
		}

		#imHeader_imMenuObject_12 .hamburger-button > div > div,
		#imStickyBar_imMenuObject_01 .hamburger-button > div > div {
			display: table-cell;
			vertical-align: middle;
		}

		#imHeader_imMenuObject_12 .hamburger-bar,
		#imStickyBar_imMenuObject_01 .hamburger-bar {
			margin: 3px auto;
			width: 65%;
			height: 3px;
			background-color: #ffffff;
		}

		/* Mobile menu hidden by default */
		#imHeader_imMenuObject_12 .menu-mobile-animated.hidden,
		#imStickyBar_imMenuObject_01 .menu-mobile-animated.hidden {
			display: none;
		}

		#imHeader_imMenuObject_12 .hamburger-menu-background-container,
		#imStickyBar_imMenuObject_01 .hamburger-menu-background-container {
			display: none;
		}

		#imHeader_imMenuObject_12 .hamburger-menu-close-button,
		#imStickyBar_imMenuObject_01 .hamburger-menu-close-button {
			display: none;
		}
		
		/* Responsive */
		@media (max-width: 959.9px) {
			#imPage {
				grid-template-columns: minmax(0, 1fr) minmax(auto, 720px) minmax(0, 1fr);
				grid-template-rows: 150px auto 200px;
			}

			#imHeaderObjects,
			#imFooterObjects,
			.imPageRow {
				width: 100%;
				max-width: 720px;
			}

			#imFooterBg {
				height: 200px;
			}

			#imFooterObjects {
				height: 200px;
			}

			#imHeader_imObjectImage_14_wrapper {
				left: 565px;
			}

			#imFooter_imObjectImage_51_wrapper {
				left: 557px;
			}
		}

		@media (max-width: 719.9px) {
			#imPage {
				grid-template-columns: minmax(0, 1fr) minmax(auto, 480px) minmax(0, 1fr);
				grid-template-rows: 120px auto 150px;
			}

			#imHeaderObjects,
			#imFooterObjects,
			.imPageRow {
				width: 100%;
				max-width: 480px;
			}

			#imFooterBg {
				height: 150px;
			}

			#imFooterObjects {
				height: 150px;
			}

			#imHeader_imObjectImage_14_wrapper {
				left: auto;
				right: 10px;
				width: 126px;
				height: 118px;
			}

			#imFooter_imObjectImage_51_wrapper {
				left: auto;
				right: 10px;
				width: 140px;
				top: 10px;
			}

			#imFooter_imObjectTitle_50_wrapper {
				top: 15px;
				left: 15px;
			}

			#imFooter_imTextObject_52_wrapper {
				top: 45px;
				left: 15px;
				width: calc(100% - 160px);
			}

			#imStickyBarContainer {
				display: none !important;
			}

			/* Show hamburger on mobile */
			#imHeader_imMenuObject_12 .hamburger-component,
			#imStickyBar_imMenuObject_01 .hamburger-component {
				display: inline-block;
			}

			/* Position hamburger button */
			#imHeader_imMenuObject_12 .hamburger-button {
				position: absolute;
				top: 20px;
				left: 10px;
				z-index: 10302;
			}

			/* Menu opened state - slides from left */
			#imHeader_imMenuObject_12-menu-opened {
				position: fixed;
				top: 0;
				bottom: 0;
				left: 0;
				width: 200px;
				z-index: 10402;
				background-color: transparent;
			}

			#imHeader_imMenuObject_12-menu-opened .hamburger-menu-background-container {
				display: block;
			}

			#imHeader_imMenuObject_12-menu-opened .hamburger-menu-background {
				position: absolute;
				display: block;
				top: 0;
				bottom: 0;
				width: 200px;
				left: 0;
