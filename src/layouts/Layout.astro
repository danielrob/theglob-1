	<script>
		// Mobile menu toggle - matches original site behavior
		document.addEventListener('DOMContentLoaded', function() {
			const menuWrapper = document.getElementById('imHeader_imMenuObject_12_wrapper');
			const menuObj = document.getElementById('imHeader_imMenuObject_12');
			const hamburgerBtns = document.querySelectorAll('.hamburger-button');
			const closeBtns = document.querySelectorAll('.hamburger-menu-close-button span');
			const mobileMenu = menuObj ? menuObj.querySelector('ul.menu-mobile-animated') : null;
			const menuBg = menuObj ? menuObj.querySelector('.hamburger-menu-background') : null;

			if (menuWrapper && menuObj) {
				// Open menu on hamburger click
				hamburgerBtns.forEach(function(btn) {
					btn.addEventListener('click', function(e) {
						e.preventDefault();
						menuWrapper.id = 'imHeader_imMenuObject_12-menu-opened';
						if (mobileMenu) mobileMenu.classList.remove('hidden');
						if (menuBg) menuBg.classList.remove('hidden');
						document.body.classList.add('im-menu-opened');
					});
				});

				// Close menu on X click
				closeBtns.forEach(function(btn) {
					btn.addEventListener('click', function(e) {
						e.preventDefault();
						menuWrapper.id = 'imHeader_imMenuObject_12_wrapper';
						if (mobileMenu) mobileMenu.classList.add('hidden');
						if (menuBg) menuBg.classList.add('hidden');
						document.body.classList.remove('im-menu-opened');
					});
				});

				// Close menu when clicking a link
				if (mobileMenu) {
					const menuLinks = mobileMenu.querySelectorAll('a');
					menuLinks.forEach(function(link) {
						link.addEventListener('click', function() {
							menuWrapper.id = 'imHeader_imMenuObject_12_wrapper';
							mobileMenu.classList.add('hidden');
							if (menuBg) menuBg.classList.add('hidden');
							document.body.classList.remove('im-menu-opened');
						});
					});
				}

				// Close on background click
				if (menuBg) {
					menuBg.addEventListener('click', function() {
						menuWrapper.id = 'imHeader_imMenuObject_12_wrapper';
						if (mobileMenu) mobileMenu.classList.add('hidden');
						menuBg.classList.add('hidden');
						document.body.classList.remove('im-menu-opened');
					});
				}
			}
		});
	</script>
